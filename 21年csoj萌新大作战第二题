//题目详见http://csoj.scnu.edu.cn/contest/30/problem/B 
#include<iostream>
using namespace std;
int main() {
	int n,m;
	cin>>n>>m;
	int arr[n];
	for(int i=0; i<n; i++) {
		cin>>arr[i];
	};
	for(int j=0; j<m; j++) {
		int a,b;
		int sum=0;
		cin>>a>>b;
		for(a,b; a<=b; a++) {
			sum = sum+arr[a-1];
		};
		cout<<sum<<endl;
	}
	return 0;
}
